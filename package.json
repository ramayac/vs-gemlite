{
  "name": "gemlite",
  "displayName": "GEM Lite",
  "description": "Syntax highlighting for simple .case/.api files used by GEM Lite",
  "icon": "images/icon.png",
  "version": "1.1.0",
  "publisher": "x188349",
  "repository": "https://github.com/",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./vscode/extension",
  "contributes": {
    "languages": [
      {
        "id": "gemlite",
        "aliases": [
          "GemLite", "GEMLite"
        ],
        "extensions": [
          ".case", ".api"
        ]
      }
    ],
    "grammars": [
      {
        "language": "gemlite",
        "scopeName": "source.notes",
        "path": "./syntaxes/gemlite.tmLanguage.json"
      }
    ]
  }
}